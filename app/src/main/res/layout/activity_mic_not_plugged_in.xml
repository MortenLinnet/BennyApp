<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/udklip"
    tools:context="com.example.morte.bennyapp.MicNotPluggedIn">

    <Button
        android:id="@+id/GoOnWithOut"
        android:layout_width="188dp"
        android:layout_height="69dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:alpha=".9"
        android:background="@drawable/micactivitybutton"
        android:backgroundTint="#FE2E2E"
        android:text="Go On without"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.963"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:onClick="GoOnWithOutMic"
        />

    <Button
        android:id="@+id/goOnWithOut"
        android:layout_width="72dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="672dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:alpha=".9"
        android:background="@drawable/micactivitybutton"
        android:backgroundTint="#2EFE2E"
        android:text="Return"
        android:onClick="Return"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

</android.support.constraint.ConstraintLayout>
